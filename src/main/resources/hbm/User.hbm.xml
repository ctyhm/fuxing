<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.koali.pojo.User" table="user" schema="" catalog="test">
        <id name="userid">
            <column name="userid" sql-type="varchar(30)" length="30"/>
        </id>
        <property name="uname">
            <column name="uname" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(15)" length="15" not-null="false"/>
        </property>
        <property name="makedate">
            <column name="makedate" sql-type="date(10)" not-null="false"/>
        </property>
        <property name="usertype">
            <column name="usertype" sql-type="varchar(8)" length="8" not-null="false"/>
        </property>
        <property name="userstate">
            <column name="userstate" sql-type="varchar(10)" length="10" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>