<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.koali.pojo.Lcgrpinsured" table="lcgrpinsured" schema="" catalog="test">
        <id name="orderid">
            <column name="orderid" sql-type="int(10)"/>
        </id>
        <property name="grpContNo">
            <column name="grpContNo" sql-type="varchar(30)" length="30" not-null="false"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20" not-null="false"/>
        </property>
        <property name="sex">
            <column name="sex" sql-type="varchar(2)" length="2" not-null="false"/>
        </property>
        <property name="age">
            <column name="age" sql-type="int(10)" not-null="false"/>
        </property>
        <property name="idnumber">
            <column name="idnumber" sql-type="varchar(20)" length="20" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>